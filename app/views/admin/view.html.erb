<html>
<body>
<script src="/closure-library/closure/goog/base.js"></script>
<script src="/javascripts/key.js"></script>
<script src="/javascripts/jquery.js"></script>


<script>

    function showTriggered(event) {
		
  	if(event.identifier=='CTRL_U')
		   {
			   redirect_short('/about');
        }
				
				if(event.identifier=='CTRL_C')
		   {
     			redirect_short('/company');

        }
				
      if(event.identifier=='CTRL_P')
		   {
     			redirect_short('/product');
        }
				
				 if(event.identifier=='CTRL_F')
		   {
     			redirect_short('/product_feature');
        }
				
		   if(event.identifier=='SHIFT_F12')
		   {
			 
     			redirect_short('/locate');
        }
				
			 if(event.identifier=='CTRL_L')
		   {
					redirect_short('/destroy_user_session');
        }
				
	 					
    }
		
	

   var shortcutHandler = new goog.ui.KeyboardShortcutHandler(document);
	 
   var CTRL = goog.ui.KeyboardShortcutHandler.Modifiers.CTRL;
   var SHIFT = goog.ui.KeyboardShortcutHandler.Modifiers.SHIFT;

	 	 
	 shortcutHandler.registerShortcut('CTRL_H',goog.events.KeyCodes.H, CTRL);
	 
	 shortcutHandler.registerShortcut('CTRL_U',goog.events.KeyCodes.U, CTRL);


	 shortcutHandler.registerShortcut('CTRL_C',goog.events.KeyCodes.C, CTRL);
	 
	 shortcutHandler.registerShortcut('CTRL_P',goog.events.KeyCodes.P, CTRL);
	 
	 
 	 shortcutHandler.registerShortcut('CTRL_F',goog.events.KeyCodes.F, CTRL);
	 
	 shortcutHandler.registerShortcut('CTRL_L',goog.events.KeyCodes.L, CTRL);

 

   shortcutHandler.registerShortcut('SHIFT_F12', 'shift+f12');

	 
		
    shortcutHandler.registerShortcut('BACK_SPACE', goog.events.KeyCodes.BACKSPACE);

   goog.events.listen(shortcutHandler,goog.ui.KeyboardShortcutHandler.EventType.SHORTCUT_TRIGGERED,showTriggered);
	
  
	$(document).ready(function(){
			$('a.menutext').click(function(){
					path=($(this).attr('href'));
				redirect_short(path);
				 return false;
			});

	});
	
	
	
	function redirect_short(path)
	{
		$.ajax({
							url: path,
							success:function(data){
								    $('#ram').html(data);
							}
						});
	}
</script>

</body>
</html>





